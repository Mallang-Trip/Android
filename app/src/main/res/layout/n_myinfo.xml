<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Myinfo_Container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Nav.Myinfo.Main_Myinfo">

    <Button
        android:id="@+id/Profile_Check"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:text="프로필 확인"
        android:textColor="#000000" />

    <Button
        android:id="@+id/Reservation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:text="나의 예약 내역"
        android:textColor="#000000" />

    <Button
        android:id="@+id/My_Text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:text="나의 작성글 내역"
        android:textColor="#000000" />

    <Button
        android:id="@+id/My_Account"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:text="수익금 또는 환불 정산금 받을 계좌 입력/변경"
        android:textColor="#000000" />

    <Button
        android:id="@+id/Resister_Driver"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:text="드라이버 등록"
        android:textColor="#000000" />

    <Button
        android:id="@+id/Fix_Myinfo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:text="개인정보 수정"
        android:textColor="#000000" />
</LinearLayout>